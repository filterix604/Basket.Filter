{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Basket.Filter.Services.CacheService": "Information",
      "Basket.Filter.Services.CatalogService": "Information"
    }
  },
  "AllowedHosts": "*",
  "Cache": {
    "EnableCaching": true,
    "UseRedis": false,
    "InMemory": {
      "MaxSizeInMB": 512,
      "DefaultExpiration": "00:30:00",
      "CatalogItemExpiration": "02:00:00"
    }
  }
}