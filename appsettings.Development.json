{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Cache": {
    "EnableCaching": true,
    "UseRedis": true,
    "InMemory": {
      "MaxSizeInMB": 100,
      "DefaultExpiration": "00:30:00",
      "CatalogItemExpiration": "02:00:00"
    },
    "Redis": {
      "ConnectionString": "localhost:6379,abortConnect=false",
      "InstanceName": "basket-filter-cache",
      "DefaultExpiration": "01:00:00",
      "CatalogItemExpiration": "06:00:00"
    }
  }
}
