{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Basket.Filter.Services.CacheService": "Debug",
      "Basket.Filter.Services.CatalogService": "Debug"
    }
  },
  "AllowedHosts": "*",
  "Cache": {
    "EnableCaching": true,
    "UseRedis": false,
    "InMemory": {
      "MaxSizeInMB": 100,
      "DefaultExpiration": "00:30:00",
      "CatalogItemExpiration": "02:00:00"
    },
    "Redis": {
      "ConnectionString": "localhost:6379",
      "InstanceName": "BasketFilter",
      "DefaultExpiration": "01:00:00",
      "CatalogItemExpiration": "06:00:00"
    }
  }
}